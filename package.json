{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "Javascript Development Environment",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check lint:watch open:src",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romikanjoy/js-dev-env.git"
  },
  "author": "Romik Guha Anjoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romikanjoy/js-dev-env/issues"
  },
  "homepage": "https://github.com/romikanjoy/js-dev-env#readme",
  "dependencies": {
    "numeral": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.16.2",
    "localtunnel": "^1.8.3",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "open": "0.0.5",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  }
}
